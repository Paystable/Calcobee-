# Enable URL rewriting
RewriteEngine On

# Redirect /ownerpanel/ to /ownerpanel
RewriteRule ^ownerpanel/$ /ownerpanel [R=301,L]

# Redirect /api/config/ to /api/config
RewriteRule ^api/config/$ /api/config [R=301,L]

# Serve index.html for all routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ index.html [QSA,L] 